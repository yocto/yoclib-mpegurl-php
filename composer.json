{
  "name": "yocto/yoclib-mpegurl",
  "type": "library",
  "description": "This yocLibrary enables your project to read and write MPEGURL files in PHP.",
  "keywords":[
	"composer",
	"hls",
	"php",
	"mpeg",
	"mpegurl",
	"yocto",
	"yoclib"
  ],
  "license": "GPL-3.0-or-later",
  "require": {
	"php": "^7||^8"
  },
  "require-dev": {
	"phpunit/phpunit": "^7||^8||^9"
  },
  "autoload": {
	"psr-4": {
	  "YOCLIB\\WBXML\\": "src/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "YOCLIB\\WBXML\\Tests\\": "tests/"
	}
  }
}